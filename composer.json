{
    "name": "yonidebleeker/webinsights",
    "keywords": [
        "laravel",
        "webinsights"
    ],
    "description": "A package to get insights on your website",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yonidebleeker\\Webinsights\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Yoni De Bleeker"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.16",
        "mobiledetect/mobiledetectlib": "^4.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yonidebleeker\\Webinsights\\WebinsightsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
